{
  "name": "@tg-app/types",
  "version": "1.0.0",
  "main": "./lib/telegram.js",
  "module": "./lib/telegram.mjs",
  "jsnext:main": "./lib/telegram.mjs",
  "typings": "./lib/telegram.d.ts",
  "exports": {
    ".": {
      "types": "./lib/telegram.d.ts",
      "import": "./lib/telegram.mjs",
      "require": "./lib/telegram.js",
      "default": "./lib/telegram.mjs"
    },
    "./global": {
      "types": "./lib/global.d.ts",
      "import": "./lib/global.mjs",
      "require": "./lib/global.js",
      "default": "./lib/global.mjs"
    }
  },
  "files": [
    "lib",
    "src",
    "README.md",
    "LICENSE.md"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "build": "tsup"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
