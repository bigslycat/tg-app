{
  "name": "@tg-app/validate-service-client",
  "version": "1.0.1",
  "description": "Client for tg-app-validate-service for frontend and Node.js",
  "main": "./lib/web-client.js",
  "module": "./lib/web-client.mjs",
  "jsnext:main": "./lib/web-client.mjs",
  "typings": "./lib/web-client.d.ts",
  "exports": {
    ".": {
      "types": "./lib/web-client.d.ts",
      "import": "./lib/web-client.mjs",
      "require": "./lib/web-client.js",
      "default": "./lib/web-client.mjs"
    },
    "./node": {
      "types": "./lib/node-client.d.ts",
      "import": "./lib/node-client.mjs",
      "require": "./lib/node-client.js",
      "default": "./lib/node-client.mjs"
    }
  },
  "files": [
    "lib",
    "src",
    "README.md",
    "LICENSE.md"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "build": "tsup"
  },
  "dependencies": {
    "@tg-app/types": "^1.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.4",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
